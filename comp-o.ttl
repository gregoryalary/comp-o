###  https://gregoryalary.github.io/comp-o#requiredPerformContract
:requiredPerformContract rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain :RequiredPerform ;
                         rdfs:range :ServiceContract ;
                         rdfs:comment "Used to link a RequiredPerform with the ServiceContract it requires."@en ;
                         rdfs:label "Required perform contract" .


